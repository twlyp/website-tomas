{
  "name": "website-tomas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "host": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.5",
    "@sveltejs/kit": "^2.50.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "svelte": "^5.47.1",
    "svelte-check": "^4.3.5",
    "svelte-gestures": "^5.2.2",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "wrangler": "^4.59.3"
  },
  "type": "module",
  "dependencies": {
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "d3-force-boundary": "^0.0.3",
    "daisyui": "^5.5.14",
    "uuid": "^13.0.0"
  },
  "packageManager": "yarn@4.7.0",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./src/app.css",
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
