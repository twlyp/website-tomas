{
  "name": "website-tomas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "host": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint .",
    "emulators:start": "firebase emulators:start --import ./emulators_data --export-on-exit"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.43.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.13",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.36.0",
    "eslint-plugin-svelte": "^3.12.4",
    "firebase-admin": "^13.5.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "svelte-gestures": "^5.2.2",
    "tailwindcss": "^4.1.13",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "d3-force-boundary": "^0.0.3",
    "daisyui": "^5.1.18",
    "firebase": "^12.3.0",
    "uuid": "^13.0.0"
  },
  "packageManager": "yarn@4.7.0",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./src/app.css",
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
